doctype html
html(lang="en")

    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Masterchef
        link(rel="icon",href="data:;base64,iVBORw0KGgo=")

        // == Stylesheets w/ Grunt Include == 
        
        // build:style inline css
        // /build

    body
        .app.w-100.d-flex.flex-row.p-0.m-0

            // ==== Left: Modules ====

            .modules.h-100.col-2.p-0
                // Title
                .title
                    span Ingredients

                // Search
                .md-form.m-0.pt-1
                    input#search.form-control(
                        type="search",
                        autocomplete="off",
                        placeholder="Search...",
                        onkeyup="search()"
                    )

                // Categories
                ul.categories.d-block
                    ul#search-results

                    li#cat-enumeration
                        a.category(href="#cat-enumeration") Enumeration
                        ul
                            li(draggable="true")
                                .name.osint HTTP Probe
                                .calories
                                    .form-group
                                        label Port
                                        input.form-control(type="number",value="80",min="1",max="65535")
                                        
                            li(draggable="true")
                                .name.osint Subdomains
                                .calories
                                    .form-group
                                        label Wordlist
                                        input.form-control(type="text")

                            li(draggable="true")
                                .name.osint Web Contents
                                .calories.d-flex.flex-row
                                    .form-group.col-8
                                        label Wordlist
                                        input.form-control(type="text")
                                    .form-group.mx-1
                                        label Extensions
                                        input.form-control(type="text")

                    li#cat-output
                        a.category(href="#cat-output") Output
                        ul
                            li(draggable="true")
                                .name Append
                                .calories
                                    .form-group
                                        label Line
                                        input.form-control(type="text")

            // ==== Middle: Recipe ====

            .kitchen.d-block.col-3.p-0
                // Title
                .title
                    span Recipe

                // Recipe
                ul#recipe
                
                // Chef
                .chef.d-flex.flex-row.justify-content-center.align-items-center
                    button#bake.btn.btn-lg.btn-block.btn-success.rounded-pill
                        img(
                            alt="Chef!",
                            src="data:image/png;base64,"
                        )
                        span Bake!

            // ==== Right: Std In/Out ====  

            .std.h-100.col.d-flex.flex-column.p-0
                // In
                .in.h-25
                    // Title
                    .title
                        span Input
                    textarea#input(tabindex="1", spellcheck="false", autofocus)

                // Out
                .out.h-75
                    // Title
                    .title
                        span Output
                    textarea#output(tabindex="1", spellcheck="false")

    // == Scripts w/ Grunt Include ==
    
